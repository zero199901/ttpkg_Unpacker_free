CREATE TABLE `bw_book` (
  `id` int NOT NULL AUTO_INCREMENT COMMENT 'ID',
  `siteid` int NOT NULL DEFAULT '0' COMMENT '渠道ID',
  `sourceid` int NOT NULL DEFAULT '0' COMMENT '渠道书籍ID',
  `booktype` int NOT NULL DEFAULT '1' COMMENT '书籍类型 1：长篇 2：短篇',
  `book_name` varchar(50) CHARACTER SET utf8mb4 COLLATE utf8mb4_0900_ai_ci NOT NULL DEFAULT '' COMMENT '书籍名称',
  `posterid` int unsigned DEFAULT '0' COMMENT '发布者ID',
  `poster` varchar(50) CHARACTER SET utf8mb4 COLLATE utf8mb4_0900_ai_ci DEFAULT NULL COMMENT '发布者',
  `authorid` bigint unsigned NOT NULL DEFAULT '0' COMMENT '作者ID',
  `author` varchar(50) CHARACTER SET utf8mb4 COLLATE utf8mb4_0900_ai_ci NOT NULL DEFAULT '' COMMENT '书籍作者',
  `masterid` int NOT NULL DEFAULT '0' COMMENT '所属编辑ID',
  `master` varchar(30) CHARACTER SET utf8mb4 COLLATE utf8mb4_0900_ai_ci DEFAULT NULL COMMENT '所属编辑',
  `intro` mediumtext CHARACTER SET utf8mb4 COLLATE utf8mb4_0900_ai_ci COMMENT '介绍',
  `thumb` varchar(255) CHARACTER SET utf8mb4 COLLATE utf8mb4_0900_ai_ci DEFAULT NULL COMMENT '封面图',
  `big_thumb` varchar(255) CHARACTER SET utf8mb4 COLLATE utf8mb4_0900_ai_ci DEFAULT NULL COMMENT '封面大图',
  `keyword` varchar(200) CHARACTER SET utf8mb4 COLLATE utf8mb4_0900_ai_ci NOT NULL DEFAULT '' COMMENT '关键词',
  `tags` varchar(200) CHARACTER SET utf8mb4 COLLATE utf8mb4_0900_ai_ci DEFAULT NULL COMMENT '标签',
  `word_count` int unsigned NOT NULL DEFAULT '0' COMMENT '字数',
  `chapter_count` int NOT NULL DEFAULT '0' COMMENT '章节数',
  `readnum` bigint unsigned NOT NULL DEFAULT '0' COMMENT '阅读数',
  `grade` varchar(10) CHARACTER SET utf8mb4 COLLATE utf8mb4_0900_ai_ci DEFAULT NULL COMMENT '等级',
  `weight` int DEFAULT '0' COMMENT '指数',
  `fullflag` tinyint unsigned NOT NULL DEFAULT '0' COMMENT '作品完结状态 =[0:连载中,1:全本]',
  `ratenum` int unsigned DEFAULT '0' COMMENT '评分次数（例如 评分9，评分次数填1）',
  `ratesum` int unsigned DEFAULT '0' COMMENT '评分总数（例如 评分9，评分总数填9）',
  `status` tinyint unsigned NOT NULL DEFAULT '0' COMMENT '审核=[0:待审核,1:通过,3:退稿,4:下架,5:建议修改]',
  `shield` tinyint unsigned NOT NULL DEFAULT '0' COMMENT '屏蔽状态=[0:未屏蔽,1:已屏蔽]',
  `sort_id` varchar(15) CHARACTER SET utf8mb4 COLLATE utf8mb4_0900_ai_ci NOT NULL COMMENT '分类ID',
  `charge_mode` tinyint unsigned NOT NULL DEFAULT '1' COMMENT 'VIP费率=[1:单章,2:单本]',
  `price` int unsigned NOT NULL DEFAULT '0' COMMENT '全本售价',
  `sale_price` int unsigned NOT NULL DEFAULT '0' COMMENT '定价',
  `copyright_id` int unsigned NOT NULL DEFAULT '0' COMMENT '版权方ID',
  `is_publish` tinyint unsigned NOT NULL DEFAULT '2' COMMENT '出版类型=[1:出版,2:原创]',
  `is_vip` tinyint unsigned NOT NULL DEFAULT '0' COMMENT '是否VIP作品=[0:否,1:是]',
  `is_sign` tinyint(1) NOT NULL DEFAULT '0' COMMENT '是否签约 0：未签约 1：已签约',
  `last_chapter_id` int NOT NULL DEFAULT '0' COMMENT '最后章节ID',
  `last_chapter_name` varchar(100) CHARACTER SET utf8mb4 COLLATE utf8mb4_0900_ai_ci DEFAULT NULL COMMENT '最后更新章节名称	',
  `free_chapter_update_time` int unsigned NOT NULL DEFAULT '0' COMMENT '免费章节更新时间',
  `vip_chapter_update_time` int unsigned NOT NULL DEFAULT '0' COMMENT 'VIP章节更新时间',
  `pulltime` int NOT NULL DEFAULT '0' COMMENT '抓取最后时间',
  `channel_type` tinyint unsigned NOT NULL DEFAULT '2' COMMENT '频道类型=[1:男频,2:女频]',
  `signtype` tinyint unsigned NOT NULL DEFAULT '0' COMMENT '签约方式=[1:买断,2:分成,3:保底+分成]',
  `buyout` varchar(10) CHARACTER SET utf8mb4 COLLATE utf8mb4_0900_ai_ci NOT NULL DEFAULT '0' COMMENT '买断金额  按千字XX元',
  `profitProportion` varchar(10) CHARACTER SET utf8mb4 COLLATE utf8mb4_0900_ai_ci NOT NULL DEFAULT '0' COMMENT '分成比例 ',
  `guaranteed` varchar(10) CHARACTER SET utf8mb4 COLLATE utf8mb4_0900_ai_ci NOT NULL DEFAULT '0' COMMENT '保底 千字XX元',
  `loweupsize` varchar(10) CHARACTER SET utf8mb4 COLLATE utf8mb4_0900_ai_ci NOT NULL DEFAULT '0' COMMENT '最低章节更新字数',
  `allsellnum` int NOT NULL DEFAULT '0' COMMENT '总销售',
  `alllikes` int NOT NULL DEFAULT '0' COMMENT '总点赞数',
  `allbonus` int NOT NULL DEFAULT '0' COMMENT '总打赏值',
  `allreview` int NOT NULL DEFAULT '0' COMMENT '总评论数',
  `create_time` int unsigned NOT NULL DEFAULT '0' COMMENT '添加时间',
  `update_time` int unsigned NOT NULL DEFAULT '0' COMMENT '更新时间',
  PRIMARY KEY (`id`) USING BTREE,
  KEY `idx_sortid` (`sort_id`) USING BTREE,
  KEY `idx_status` (`status`) USING BTREE
) ENGINE=InnoDB AUTO_INCREMENT=9 DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_0900_ai_ci ROW_FORMAT=DYNAMIC COMMENT='书库';